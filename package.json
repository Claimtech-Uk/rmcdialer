{
  "name": "dialler-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "postinstall": "npm run db:generate:all",
    "vercel-build": "npm run db:generate:all && next build",
    "db:generate": "prisma generate",
    "db:generate:replica": "prisma generate --schema=prisma/replica.prisma",
    "db:generate:all": "npm run db:generate && npm run db:generate:replica",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:studio:replica": "prisma studio --schema=prisma/replica.prisma",
    "mysql:test": "tsx -e \"import('./lib/mysql.js').then(m => m.testReplicaConnection())\"",
    "mysql:stats": "tsx -e \"import('./lib/mysql.js').then(m => m.getReplicaStats().then(console.log))\"",
    "deploy": "./scripts/deploy.sh",
    "deploy:force": "npx vercel --prod",
    "check-deployments": "npx vercel ls"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@twilio/voice-sdk": "^2.11.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.14.2",
    "next": "^14.0.4",
    "prisma": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "redis": "^4.6.12",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^4.23.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.32",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3"
  }
}
