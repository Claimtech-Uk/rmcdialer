{
  "name": "dialler-system",
  "version": "1.0.0",
  "description": "RMC Dialler System - Standalone dialler for proactive claim completion",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@dialler/shared && turbo run build --filter=@dialler/web",
    "start": "turbo run start",
    "test": "turbo run test",
    "test:integration": "turbo run test:integration",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "migrate": "cd apps/api && npx prisma migrate deploy",
    "db:generate": "cd apps/api && npx prisma generate",
    "db:seed": "cd apps/api && npx prisma db seed",
    "deploy": "vercel --prod",
    "deploy:staging": "vercel"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "turbo": "^1.10.16",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resolvemyclaim/dialler-system.git"
  },
  "keywords": [
    "dialler",
    "claims",
    "twilio",
    "contact-center",
    "queue-management"
  ],
  "author": "Resolve My Claim",
  "license": "UNLICENSED"
} 